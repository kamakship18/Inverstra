# Fetch.ai Environment Configuration

# Fetch.ai Network Configuration
FETCHAI_NETWORK_NAME=fetchhub-4
FETCHAI_CHAIN_ID=fetchhub-4
FETCHAI_RPC_URL=https://rpc-fetchhub.fetch.ai:443
FETCHAI_REST_URL=https://rest-fetchhub.fetch.ai

# Agent Configuration
FETCHAI_AGENT_WALLET_PATH=./wallets/agent_wallet.json
FETCHAI_AGENT_PASSWORD=your_agent_password_here

# API Keys (if using external services)
PERPLEXITY_API_KEY=your_perplexity_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Agent Performance Settings
FETCHAI_MAX_CONCURRENT_AGENTS=5
FETCHAI_AGENT_TIMEOUT=30000
FETCHAI_QUALITY_THRESHOLD=70

# Token Economy Settings
FETCHAI_REWARD_BASE_AMOUNT=2
FETCHAI_QUALITY_MULTIPLIER=0.1
FETCHAI_MAX_REWARD_PER_TASK=10

# Database Settings (if using separate DB for Fetch.ai)
FETCHAI_DB_URL=mongodb://localhost:27017/fetchai_agents
FETCHAI_DB_NAME=fetchai_agents

# Logging
FETCHAI_LOG_LEVEL=info
FETCHAI_ENABLE_METRICS=true

# Security
FETCHAI_ENCRYPTION_KEY=your_encryption_key_here
FETCHAI_SIGNATURE_VERIFICATION=true

# Development Settings
FETCHAI_DEV_MODE=true
FETCHAI_MOCK_AGENTS=false
FETCHAI_SIMULATE_DELAYS=true
